local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/naypramx/Ui__Project/Script/XeNonUi", true))()

local CenterHubNo1 = library:CreateWindow("Nestle Hub - Kaitan",Enum.KeyCode.RightControl)

