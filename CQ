CheckStatus()

Settings:AddToggle("Auto Update Status",_G.UpdateStatus,function(vu)
	_G.UpdateStatus = vu
end)
spawn(function()
	while wait(.5) do
		pcall(function()
			if _G.UpdateStatus then
				CheckStatus()
				wait(_G.Delay)
			else
				wait(2)
			end
		end)
	end
end)
